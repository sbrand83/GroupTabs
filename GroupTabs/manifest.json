{
  "manifest_version": 2,

  "name": "Group Tabs",
  "description": "Quickly open groups of commonly used tabs.",
  "version": "1.0.1",

  "icons": { "16": "icons/icon_16.png",
             "48": "icons/icon_48.png",
             "128": "icons/icon_128.png" },

  "browser_action": {
    "default_icon": "icons/icon_19.png",
    "default_popup": "popup.html",
    "default_title": "GroupTabs"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}