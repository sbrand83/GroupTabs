{
  "manifest_version": 2,

  "name": "GroupTabs",
  "description": "Quickly open groups of commonly used tabs.",
  "version": "1.0",

  "icons": { "16": "icon_16.png",
             "48": "icon_48.png",
             "128": "icon_128.png" },

  "browser_action": {
    "default_icon": "icon_19.png",
    "default_popup": "popup.html",
    "default_title": "GroupTabs"
  },
  "permissions": [
    "storage",
    "tabs"
  ]
}